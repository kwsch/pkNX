include "PokeDropItem.fbs";
namespace pkNX.Structures.FlatBuffers.Arceus;
attribute "fs_serializer";

table PokeMisc {
    Species:int;
    Form:int;
    Field_02:int;
    ScaleFactor:float = 1; // scale factor when not alpha
    AlphaScaleFactor:float = 2; // scale factor when alpha (forced 255)
    Field_05:int = 1; // similar scale amplification like Field 6, but not used for levels
    OybnLevelIndex:int = 2;

    DropTableRef:ulong;
    DropTable:PokeDropItem; // not required
    AlphaDropTableRef:ulong;
    AlphaDropTable:PokeDropItem; // not required

    /// "pm0000_00_00"
    Value:string (required);

    /// = { 0, 1, 2, 3 };
    Field_10:[uint] (required);
    Field_11:int = 0;
    Field_12:int = 4;
    Field_13:int = 10;
    Field_14:bool = false;
    Field_15:int = 0;
    Field_16:float = 0.01;
    Field_17:float = 1.0;
    Field_18:bool = true; // false for zubat, psyduck
    Field_19:float = 0;
    Field_20:float = 1.0;
    Field_21:uint; // None have this
    Field_22:float;
}

table PokeMiscTable (fs_serializer) {
    Table:[PokeMisc] (required);
}

root_type PokeMiscTable;
